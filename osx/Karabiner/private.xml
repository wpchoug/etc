<?xml version="1.0"?>
<!-- ~/Library/Application\ Support/Karabiner/private.xml -->
<root>
  <appdef>
    <appname>VIRTUALBOX</appname>
    <equal>org.virtualbox.app.VirtualBoxVM</equal>
  </appdef>

  <!-- Apple Keyboard Start -->
  <!-- Hyper -->
  <item>
    <name>Control_L to Hyper(cmd+ctl+opt)</name>
    <identifier>private.control_l2hyper</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::CONTROL_L,
      KeyCode::COMMAND_L, ModifierFlag::OPTION_L | ModifierFlag::CONTROL_L
    </autogen>
  </item>

  <!-- Apple Keyboard End -->

  <!-- Non-Apple Keyboard Start -->
  <!-- Hyper -->
  <item>
    <name>Command_L to Hyper(cmd+ctl+opt), except VirtualBox</name>
    <identifier>private.commandL2hyper</identifier>
    <not>VIRTUALBOX</not>
    <autogen>
      __KeyOverlaidModifier__
      KeyCode::COMMAND_L,
      KeyCode::COMMAND_L, ModifierFlag::OPTION_L | ModifierFlag::CONTROL_L,
      KeyCode::S
    </autogen>
  </item>

  <item>
    <name>PC Application to Hyper(cmd+ctl+opt)</name>
    <identifier>private.application2hyper</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::PC_APPLICATION,
      KeyCode::COMMAND_R, ModifierFlag::OPTION_R | ModifierFlag::CONTROL_R
    </autogen>
  </item>

  <!-- Alt to cmd -->
  <item>
    <name>Alt_L to Cmd_L and ctl+space, except VirtualBox</name>
    <identifier>private.altL2commandL_controlSpace</identifier>
    <not>VIRTUALBOX</not>
    <autogen>
      __KeyOverlaidModifier__
      KeyCode::OPTION_L,
      KeyCode::COMMAND_L,
      KeyCode::SPACE, ModifierFlag::CONTROL_L
    </autogen>
  </item>
  <item>
    <name>Alt_R to Cmd_R and cmd+opt+space, except VirtualBox</name>
    <identifier>private.altR2commandR_commandOptionSpace</identifier>
    <not>VIRTUALBOX</not>
    <autogen>
      __KeyOverlaidModifier__
      KeyCode::OPTION_R,
      KeyCode::COMMAND_R,
      KeyCode::SPACE, ModifierFlag::COMMAND_R | ModifierFlag::OPTION_R
    </autogen>
  </item>

  <!-- cmd to alt -->
  <item>
    <name>Cmd_L to Alt_L, except VirtualBox</name>
    <identifier>private.cmdL2altL</identifier>
    <not>VIRTUALBOX</not>
    <autogen>
      __KeyToKey__
      KeyCode::COMMAND_L,
      KeyCode::OPTION_L
    </autogen>
  </item>

  <!-- Non-Apple Keyboard End -->

